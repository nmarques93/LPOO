package maze.logic;

import java.awt.Point;

public class Element {
	protected posx, posy;
	protected char symbol;
	
	public char getSymbol(){
		return this.symbol;
	}
	
	public void setSymbol(char s){
		symbol=s;
	}
	
	/*public int getX(){
		return posx;
	}
	
	public int getY(){
		return posy;
	}
	
	public void setX(int x){
		this.posx=x;
	}
	
	public void setY(int y){
